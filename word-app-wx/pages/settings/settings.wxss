.page-root{min-height:100vh;background:var(--app-bg-page-gradient,var(--app-bg-page))}.page-body{padding:32rpx 32rpx 120rpx}.setting-group{background:var(--app-glass-bg);backdrop-filter:var(--app-glass-blur);-webkit-backdrop-filter:var(--app-glass-blur);border-radius:24rpx;margin-bottom:32rpx;border:1rpx solid var(--app-glass-border);overflow:hidden}.setting-group-title{display:flex;align-items:center;gap:12rpx;padding:24rpx 32rpx;background:var(--td-gray-color-1);font-size:24rpx;font-weight:700;color:var(--app-text-secondary)}.setting-row{display:flex;align-items:center;justify-content:space-between;padding:32rpx;border-bottom:1rpx solid var(--td-component-stroke);font-size:28rpx;font-weight:600;color:var(--app-text-primary)}.setting-row:last-child{border-bottom:none}.setting-row .value{color:var(--app-text-secondary);font-size:26rpx}.logout-btn{background:var(--app-glass-bg);backdrop-filter:var(--app-glass-blur);-webkit-backdrop-filter:var(--app-glass-blur);border:1rpx solid var(--app-danger-border);color:var(--app-danger);padding:32rpx;text-align:center;font-size:32rpx;font-weight:700;border-radius:24rpx;margin-bottom:48rpx}.version-block{display:flex;flex-direction:column;align-items:center;gap:8rpx;margin-top:48rpx}.version-logo{width:96rpx;height:96rpx;background:var(--td-bg-color-container);border-radius:24rpx;box-shadow:0 4rpx 16rpx rgba(0,0,0,.06);display:flex;align-items:center;justify-content:center;margin-bottom:8rpx}.version-logo-inner{width:64rpx;height:64rpx;background:var(--app-primary);border-radius:16rpx}.version-text{font-size:24rpx;font-weight:600;color:var(--app-text-secondary)}.version-build{font-size:20rpx;color:var(--app-text-secondary);opacity:.6}
